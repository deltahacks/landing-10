---
import Layout from "../layouts/main.astro";
// Component Imports
// import Button from '../components/Button.astro';
import Hero from "../components/Hero.astro";
import WhoAreWe from "../components/WhoAreWe.astro";
import Sponsors from "../components/Sponsors.astro";
import Navbar from "../components/Navbar.astro";
import FAQ from "../components/FAQ.astro";
import CoundownAndLinks from "../components/CountdownAndLinks.astro";
import Footer from "../components/Footer.astro";

// Full Astro Component Syntax:
// https://docs.astro.build/core-concepts/astro-components/
---

<Layout>
  <div class="fixed top-10 z-50 box-content w-full">
    <Navbar />
  </div>
  <Hero />
  <WhoAreWe />
  <p class="px-10 pt-10">
    This site is still under construction! If you stumble across this, please
    check back later! orz
  </p>
  <div class="flex w-full flex-col items-center">
    <Sponsors />
    <FAQ />
  </div>
  <CoundownAndLinks />
  <Footer />
</Layout>
